{
    "name": "londry/flarum-auth-whmcs",
    "description": "Allow users to log in with Your WHMCS.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason long",
            "email": "tang@londry.cn"
        }
    ],
    "support": {
        "issues": "https://github.com/flarum/core/issues",
        "source": "https://github.com/flarum/flarum-ext-auth-github"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6",
        "londry/oauth2-whmcs": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\Auth\\WHMCS\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "WHMCS Login",
            "icon": {
                "name": "google",
                "backgroundColor": "#eee",
                "color": "green"
            }
        }
    }
}
